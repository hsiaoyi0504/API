{
  "manifest_version": 2,

  "name": "API test",
  "description": "This extension shows a updated Google Youtube search result for you",
  "version": "2.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy":"script-src 'self'  'unsafe-eval' https://apis.google.com https://ajax.googleapis.com; object-src 'self';",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
  
  
}
